# Note: this container will have the name wupanhao/lepi_xxx
# docker build . -f lepi_xxx -t wupanhao/lepi_xxx:v0.1
FROM wupanhao/lepi_driver:v0.1

RUN apt install -y libpng16-16 libjpeg62

WORKDIR /root/deps

COPY ./build ./

RUN pip install ./face_recognition_models-0.3.0-py2.py3-none-any.whl ./dlib-19.18.0-cp27-cp27mu-linux_armv7l.whl face_recognition
